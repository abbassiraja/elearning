
{% extends 'base.html.twig' %}

{% block title %} Home{% endblock %}

{% block body %}
<style>
/*style qui sommes nous*/

@import url('https://fonts.googleapis.com/css2?family=Lora:ital,wght@0,500;0,600;1,500&family=Montserrat&display=swap');

*,
*::before,
*::after {
  margin:0;
  padding:0;
  box-sizing:border-box;
}
body {
  font:400 1rem/1.4 'Montserrat',sans-serif;
  
  background-size:cover;
  background-repeat:no-repeat;
}
.card--container {
  width:80%;
  margin:1em auto;
}
.profile__card {
  min-width:100%;
  background-color:#fff;
  box-shadow:2px 2px 5px rgba(0,0,0,.4);
  display:grid;
  grid-template-columns:1fr;
  grid-template-areas:'header'
                      'image'
                       'body';
  place-items:Center;
}
.card__header {
  grid-area:header;
  background-color:#010d20;
  min-width:100%;
  border-bottom:2px dashed #fff;
}
.card__header h2 {
  font-weight:600;
  font-family:'Lora',sans-serif;
  font-size:1.8rem;
  text-align:Center;
  color:#fff;
  padding:.3em 0;
}
.image__container {
  padding:1.5em 0 2em;
  background-color:#001230;
  width:100%;
  height:100%;
  clip-path:polygon(0 0,100% 0,100% 100%,97% 93%,94% 100%,91% 93%,88% 100%,84% 93%,81% 100%,78% 93%,75% 100%,72% 93%,69% 100%,66% 93%,63% 100%,60% 93%,57% 100%,54% 93%,51% 100%,48% 93%,45% 100%,42% 93%,38.5% 100%,36% 93%,33% 100%,30% 93%,27% 100%,24% 93%,21% 100%,18% 93%,15% 100%,12% 93%,9% 100%,6% 93%,3% 100%,0 93%);
  display:flex;
  align-items:Center;
  justify-content:Center;
}
.card__image {
  width:200px;
  height:200px;
  background-color:blue;
  border-radius:50%;
  box-shadow:0 0 5px rgba(0,0,0,.5)
}
.card__image img {
  min-width:100%;
  border-radius:50%;
  object-fit:cover;
}

.about__me {
  color:#7b7d84;
  font-weight:500;
  font-size:.9rem;
  padding:1em;  
  border-bottom:3px dashed black;
}
.important__social__links {
 background-color:#187de7;
  padding:.5em;
  display:flex;
  justify-content:space-evenly;
  font-size:1.5rem;
}

i {
  background-color:#fff;
  padding:.5em;
  border-radius:50%;
  box-shadow:5px 5px 5px rgba(0,0,0,.1) inset ,2px 2px 2px rgba(0,0,0,.1),-2px -2px 2px rgba(0,0,0,.1);
  color:#1f1d01;
  transition:background-color 300ms,color 300ms;
}
.fa-facebook {
  padding:.5em .68em;
}
a{
  position:relative;
}
a::before {
  --content:'facebook';
  --content-back:#0d7ff2;
  content:var(--content);
  font-size:15px;
  text-align:Center;
  color:#fff;
  font-weight:600;
  position:absolute;
  top:-50px;
  left:-35px;
  width:110px;height:30px;
  background-color:var(--content-back);
  border-radius:15px 15px 0 0;
  clip-path:polygon(0 0,100% 0,100% 80%,54% 80%,52% 100%,50% 100%,48% 80%,0 80%);
  opacity:0;
  transition:opacity 400ms;
}
a:nth-of-type(1)::before {
  --content:'github';
  --content-back:rgba(0,0,0,.9);
}
a:nth-of-type(2)::before {
  --content:'codepen';
  --content-back:rgba(0,0,0,.9);
} 
a:nth-of-type(3)::before {
  --content:'twitter';
  --content-back:#0049ff;
}
a:hover::before,
a:focus::before   {
  opacity:1;
  transition:400ms;
}



@media screen and (min-width:920px) {
  .profile__card {
  grid-template-columns:repeat(2,1fr);
  grid-template-areas:'header header'
                      'image body';
  }
  .image__container {
    clip-path:none;
  }
  .about__me {
    padding:1.25em 1em;
    font-size:1rem;
  }
  .important__social__links {
  font-size:1.4rem;
  }
}



</style>


